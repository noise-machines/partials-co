{
  "hosting": {
    "public": "build",
    "cleanUrls": true,
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "headers": [{
      "source" : "**/service-worker.js",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=0"
      }]
    }, {
        "source" : "static/js/**/*.js",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "max-age=3600"
        }]
    }]
  }
}
